<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" href="https://www.santafe.gob.ar/assets/standard/images/favicon.ico">
    <!-- end Required meta tags -->
    {% block head %}
        <title>{{ datos_encabezado_title | default('Titulo de la aplicación') }}</title>    
    {% endblock %} 

    {% block css %}

        {% if configuration_offline == true %}

            {# asset locales #}
            <link rel="stylesheet" href="{{ asset('bundles/stgtheme/css/custom-materialize.css' ~ '?version=012020') }}">

            <!-- assets fonts -->
            <link rel="stylesheet" href="https://www.santafe.gob.ar/assets/standard/css/fonts.css" type="text/css">


            <style>

                @font-face {
                    font-family: 'Material Icons';
                    font-style: normal;
                    font-weight: 400;
                    src: url({{ asset('bundles/stgtheme/assets/material-design-icons/MaterialIcons-Regular.eot')}}); /* For IE6-8 */
                    src: local('Material Icons'),
                    local('MaterialIcons-Regular'),
                    url({{ asset('bundles/stgtheme/assets/material-design-icons/MaterialIcons-Regular.woff2') }}) format('woff2'),
                    url({{ asset('bundles/stgtheme/assets/material-design-icons/MaterialIcons-Regular.woff') }}) format('woff'),
                    url({{ asset('bundles/stgtheme/assets/material-design-icons/MaterialIcons-Regular.ttf') }}) format('truetype');
                }

                .material-icons {
                    font-family: 'Material Icons';
                    font-weight: normal;
                    font-style: normal;
                    font-size: 24px;
                    line-height: 1;
                    letter-spacing: normal;
                    text-transform: none;
                    display: inline-block;
                    white-space: nowrap;
                    word-wrap: normal;
                    direction: ltr;
                    -moz-font-feature-settings: 'liga';
                    -moz-osx-font-smoothing: grayscale;
                }


            </style>


        {% else %}

            {# assets con cdn externos #}
            <link rel="stylesheet" href="{{ asset('bundles/stgtheme/css/custom-materialize.css' ~ '?version=012020') }}">

            <!-- assets fonts -->
            <link rel="stylesheet" href="https://www.santafe.gob.ar/assets/standard/css/fonts.css" type="text/css">

            <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
                  rel="stylesheet">

        {% endif %}


    {% endblock %}
</head>
<body>

{% block nav %}
    <div class="navbar-fixed">

        {% block navItemsDropdown %}
            <ul id="dropdown1" class="dropdown-content">
                <li><a href="#!">one</a></li>
                <li><a href="#!">two</a></li>
                <li class="divider"></li>
                <li><a href="#!">three</a></li>
                <li><a href="#!"><i class="material-icons">view_module</i>four</a></li>
                <li><a href="#!"><i class="material-icons">cloud</i>five</a></li>
            </ul>
        {% endblock %}

        <nav>
            <div class="nav-wrapper">

                {% block navBar %}
                    <a href="https://www.santafe.gob.ar" class="brand-logo"><img
                                src="https://www.santafe.gob.ar/assets/standard/images/gob-santafe.png"></a>
                    <ul class="right hide-on-med-and-down">
                        {% block navBarItems %}

                            <li><a href="#">Sass</a></li>
                            <li><a href="#">Components</a></li>
                            <li><a href="#"><i class="material-icons left">search</i>Link with Left Icon</a>
                            </li>
                            <li><a href="#"><i class="material-icons right">view_module</i>Link with Right
                                    Icon</a></li>
                            <li><a class="dropdown-trigger" href="#!" data-target="dropdown1">Dropdown<i
                                            class="material-icons right">arrow_drop_down</i></a></li>
                        {% endblock %}

                    </ul>
                {% endblock %}
                <a href="#" data-target="slide-out" class="sidenav-trigger hide-on-large-only"><i class="material-icons">menu</i></a>
           </div>

        </nav>

    </div>
                {% block sideNav %}
                    <ul id="slide-out" class="sidenav">
                        {% block sideItems %}
                            <li>
                                <div class="user-view">
                                    <div class="background">
                                        <img src="{{ asset('bundles/stgtheme/img/office.jpg') }}">
                                    </div>
                                    <a href="#!user"><img class="circle"
                                                          src="{{ asset('bundles/stgtheme/img/person.png') }}"></a>
                                    <a href="#!name"><span class="white-text name">Name of person</span></a>
                                    <a href="#!email"><span class="white-text email">person@gmail.com</span></a>
                                </div>
                            </li>
                            <li><a href="#!"><i class="material-icons">cloud</i>First Link With Icon</a></li>
                            <li><a href="#!">Second Link</a></li>
                            <li>
                                <div class="divider"></div>
                            </li>
                            <li><a class="subheader">Subheader</a></li>
                            <li><a class="waves-effect" href="#!">Third Link With Waves</a></li>
                        {% endblock %}
                    </ul>
                    
                {% endblock %}

            
{% endblock %}

{% block main %}
    <main role="main" class="container">


        <div class="row">

            <div class="col s12 l12">
                <h1>{{ datos_encabezado_title | default('Titulo de la aplicación') }}</h1>

                <p>{{ datos_encabezado_entity | default('Organismo o dependencia') }}</p>

                {% block cover %}
                    <div class="cover">
                    </div>
                {% endblock %}
            </div>

        </div>

        {% block breadcrumb %}
            <div class="row">
                <div class="col s12">
                    <nav>
                        <div class="nav-wrapper">
                            <div class="col s12">
                                {% block breadcrumbItems %}
                                    <a href="#!" class="breadcrumb">First</a>
                                    <a href="#!" class="breadcrumb">Second</a>
                                    <a href="#!" class="breadcrumb">Third</a>
                                {% endblock %}
                            </div>
                        </div>
                    </nav>
                </div>
            </div>
        {% endblock %}

        {% block content %}


        <div class="card-panel">
            <h4 class="header2">Alerts</h4>

            <div class="row">
                <div class="col s12 m6">
                    <div class="card red lighten-2">
                        <div class="card-content white-text">
                            <span class="card-title"><i class="material-icons prefix white-text">error</i> Errores</span>
                            <p>1. Username cant be empty</p>
                            <p>2. Password cant be empty</p>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col s12 m6">
                    <div class="card blue lighten-2 ">
                        <div class="card-content white-text">
                            <span class="card-title"><i class="material-icons prefix white-text">info</i> Información</span>
                            <p>Information content...</p>
                        </div>
                    </div>
                </div>
            </div>


            <div class="row">
                <div class="col s12 m6">
                    <div class="card orange lighten-4">
                        <div class="card-content">
                            <span class="card-title"><i class="material-icons prefix">warning</i> Alerta</span>
                            <p>warning content...</p>
                        </div>
                    </div>
                </div>
            </div>

        </div>


            <div class="card-panel">
                <h4 class="header2">Basic Form</h4>

                <div class="row">
                    <form id="formulario-demo-materializecss-stg" class="col s12">

                        <div class="row">
                            <div class="input-field col m12 s12 l6">
                                <i class="material-icons prefix">email</i>
                                <label for="inputEmail" class="">E-Mail *</label>
                                <input id="inputEmail" name="inputEmail" data-error=".errorInputEmail" type="email">
                                <div class="errorInputEmail">
                                </div>

                            </div>
                            <div class="input-field col m12 s12 l6">
                                <i class="material-icons prefix">account_circle</i>
                                <input type="password" class="form-control" id="inputPassword4">
                                <label for="inputPassword4">Password</label>

                            </div>
                        </div>

                        <div class="row">
                            <div class="input-field col m12 s12 l12">
                                <i class="material-icons prefix">home</i>
                                <input type="text" class="form-control" id="inputAddress">
                                <label for="inputAddress">Address</label>
                            </div>
                        </div>

                        <div class="row">
                            <div class="input-field col m12 s12 l6">
                                <i class="material-icons prefix">location_city</i>
                                <label for="inputCity">City</label>
                                <input type="text" class="form-control" id="inputCity">
                            </div>


                            <div class="input-field col m12 s12 l6">
                                <i class="material-icons prefix">location_on</i>
                                <label for="inputZip">Zip</label>
                                <input type="text" class="form-control" id="inputZip">
                            </div>


                        </div>

                        <div class="row">
                            <div class="input-field col s12">
                                <i class="material-icons prefix">mode_edit</i>
                                <textarea id="icon_prefix2" class="materialize-textarea"></textarea>
                                <label for="icon_prefix2">First Name</label>
                            </div>
                        </div>

                        <div class="row">

                            <div class="col m12 s12">
                                <label>
                                <input type="checkbox" id="test5"/>
                                <span>Confirm</span>
                                </label>
                            </div>

                        </div>


                        <div class="row">
                            <div class="col m12 s12 buttons-custom-responsive">
                                <button type="submit" class="btn btn-primary">Aceptar</button>
                                <a type="submit" class="btn btn-primary">Aceptar Link</a>
                            </div>
                        </div>

                    </form>
                </div>
            </div>

        {% endblock %}

        {% block backTop %}

            <div class="row">
                <div class="col m12">
                    <p class="right-align">
                        <a href="#"><i class="material-icons">keyboard_arrow_up</i></a>
                    </p>
                </div>
            </div>

        {% endblock %}


    </main>
{% endblock %}

<div class="clearfix"><br></div>

{% block footer %}
     <footer class="page-footer footer text-muted">
	<div class="container">
		<div class="row">

			<div class="col s12 col m4">
				<div class="sociales">
					<p>
						<a href="//www.santafe.gob.ar/index.php/web/content/view/full/117678">RSS / SUSCRIPCIÓN A NOTICIAS</a>
					</p>
					<ul class="list-inline footer-ul">
						<li class="list-inline-item">
							<a target="_blank" href="http://www.twitter.com/GobSantaFe">
								<i class="icon-footertwitter"></i>
							</a>
						</li>
						<li class="list-inline-item">
							<a target="_blank" href="http://www.facebook.com/GobSantaFe">
								<i class="icon-footerfacebook"></i>
							</a>
						</li>
						<li class="list-inline-item">
							<a target="_blank" href="http://www.youtube.com/GobSantaFe">
								<i class="icon-footeryoutube"></i>
							</a>
						</li>
					</ul>
				</div>
			</div>
			<div class="col s12 col m4">
				<div class="sociales" style="text-align:center">					
					<img src="//www.santafe.gob.ar/assets/standard/images/marca-footer.png" alt="Santa Fe">
				</div>
			</div>
			<div class="col s12 col m4">
				<div class="sociales marca">
					<p>Atención telefónica: 0800-777-0801
					</p>
					<p>Lunes a viernes de 8 a 18 hs
					</p>
					<p>
						<span class="cc">c</span>
						Atribución-CompartirIgual 2.5 Argentina</p>
				</div>
			</div>
		</div>

	</div>
</footer>
{% endblock %}

{% block javascript %}

    {% if configuration_offline == true %}
        <!-- Compiled and minified JavaScript -->
        <script src="{{ asset('bundles/stgtheme/assets/jquery/jquery-3.5.1.slim/jquery-3.5.1.min.js') }}"></script>
        <!-- materialize -->
        <script src="{{ asset('bundles/stgtheme/assets/materialize/1.0.0/materialize.min.js') }}"></script>
        <!-- validation -->
        <script src="{{ asset('bundles/stgtheme/assets/jquery-validate/1.19.2/jquery.validate.min.js') }}"></script>
        <script src="{{ asset('bundles/stgtheme/assets/jquery-validate/1.19.2/additional-methods.min.js') }}"></script>
        <script src="{{ asset('bundles/stgtheme/assets/jquery-validate/1.19.2/localization/messages_es_AR.min.js') }}"></script>

    {% else %}
        <!-- Compiled and minified JavaScript -->
        <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"  integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
        <!-- materialize -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
        <!-- validation -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/jquery.validate.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/additional-methods.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/localization/messages_es_AR.min.js"></script>

    {% endif %}

{% block ready %}

    <script>

        $(function () {

            M.AutoInit();
            $(".sidenav").sidenav();
            $(".dropdown-trigger").dropdown();
            

            //validate
            $("#formulario-demo-materializecss-stg").validate({
                rules: {
                    inputEmail: {
                        required: true,
                        email: true
                    },
                },
                //For custom messages
                messages: {
                    inputEmail: {
                        required: "Este campo es requerido",
                        email: "Ingrese un email correcto"
                    }
                },
                errorElement: 'div',
                errorPlacement: function (error, element) {

                    var placement = $(element).data('error');

                    if (placement) {
                        $(placement).append(error)
                    } else {
                        error.insertAfter(element);
                    }
                }
            });


        });

    </script>


{% endblock %}

{% endblock %}

</body>
</html>